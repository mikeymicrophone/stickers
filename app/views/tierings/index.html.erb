<h1>Listing tierings</h1>

<table>
  <tr>
    <th>Endeavor</th>
    <th>Tier</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tierings.each do |tiering| %>
  <tr>
    <td><%= tiering.endeavor %></td>
    <td><%= tiering.tier %></td>
    <td><%= link_to 'Show', tiering %></td>
    <td><%= link_to 'Edit', edit_tiering_path(tiering) %></td>
    <td><%= link_to 'Destroy', tiering, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Tiering', new_tiering_path %>
